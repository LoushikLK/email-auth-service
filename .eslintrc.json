{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "import"],
  "rules": {
    "indent": ["off", "tab"],
    "no-tabs": 0,
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "import/no-default-export": "error"
  }
}
